<header class="cabezera">
  <nav class="nav">
    <a id="logo" href="/"
      ><img src="/recursos/imagenes/logo.svg" alt="logo wooplance" />
    </a>

    <div class="grupo_buscar">
      <div class="grupo_categorias">
        <button id="btn_categorias" class="item_cat">Categorias</button>
      </div>
      <div class="grupo_busqueda">
        <form action="/search" method="GET">
          <input id="input_buscar" type="text" name="search" placeholder="EJ: 'Web Developer' " >
          <button id="btn_buscar" type="submit"><i class="bx bx-search"></i></button>
        </form>
      </div>
    </div>

    <div class="grupo_entrar">
      <a id="btn_registrarse" href="/gig/add">Publicar</a>
      <a href="/profile/id/<%= myUser.id %> " class="profile-link">
        <img
          class="img-fluid"
          style="
            width: 40px;
            height: 40px;
            padding-right: 5px;
            border-radius: 50%;
          "
          src="<%= myUser.profilePic%>"
          alt=""
        />
        <p><%= myUser.name%> <%= myUser.lastName%> </p>
      </a>
    </div>
  </nav>
</header>
